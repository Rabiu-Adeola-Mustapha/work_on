{
  "name": "her-node",
  "version": "1.0.0",
  "main": "src/app.ts",
  "license": "MIT",
  "scripts": {
    "watch-test": "concurrently \"tsc -w\" \"cross-env NODE_ENV=test DEBUG='project:*' TZ=UTC nodemon ./dist/app.js\"",
    "watch-dev": "concurrently \"tsc -w\" \"cross-env NODE_ENV=development DEBUG='project:*' TZ=UTC nodemon ./dist/app.js\"",
    "watch-node": "cross-env DEBUG=project:* TZ=UTC nodemon ./dist/app.js",
    "watch-node-test": "cross-env NODE_ENV=test DEBUG=project:* TZ=UTC nodemon ./dist/app.js",
    "watch-ts": "tsc -w",
    "task": "cross-env NODE_ENV=test TZ=UTC node ./dist/tasks/taskStart",
    "test": "tsc && cross-env NODE_ENV=test DEBUG='project:*' TZ=UTC mocha dist/test/*.js --timeout 20000 --exit --bail",
    "lint": "eslint src/**",
    "build": "rm -rf ./build-dist/* && tsc --outdir build-dist/build && cp package.json build-dist/package.json && cp yarn.lock build-dist/yarn.lock && cp Dockerfile build-dist/Dockerfile"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.210.0",
    "@aws-sdk/lib-storage": "^3.210.0",
    "axios": "^1.2.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "google-auth-library": "^8.7.0",
    "handlebars": "^4.7.7",
    "image-size": "^1.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "remove": "^0.1.5",
    "sharp": "^0.31.2",
    "short-uuid": "^4.2.2",
    "source-map-support": "^0.5.21",
    "stripe": "^11.11.0",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.6",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.12",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "chai": "^4.3.6",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "mocha": "^10.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "supertest": "6.2.4",
    "typescript": "*"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
